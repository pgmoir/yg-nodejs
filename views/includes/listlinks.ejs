<div class="card">
  <div class="card-body">
    <h3 class="card-title h5"><%= card.title %></h3>
    <div class="card m-0">
      <ul class="list-group list-group-flush">
        <% for (let link of card.links) { %>
        <li class="list-group-item"><a href="<%= link.url %>" class="<%= link.icon %>"><span><%= link.title %></span></a></li>
        <% } %>
      </ul>
    </div>
  </div>
</div>

